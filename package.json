{
  "name": "hello-expense-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.4",
    "googleapis": "^39.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "client": "PORT=${PORT:-$CLIENT_PORT} react-scripts start",
    "server": "PORT=${PORT:-$SERVER_PORT} nodemon --watch server --watch shared --exec 'ts-node --files --project tsconfig.server.json' server/index.ts",
    "dev": "SERVER_PORT=8001 CLIENT_PORT=8000 concurrently -n client,server -i --default-input-target server -c green,blue --kill-others-on-fail \"npm run client\" \"npm run server\"",
    "start": "node .build/server/index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "react-scripts build",
    "build:server": "tsc --project tsconfig.server.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:8000/",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/gapi": "0.0.36",
    "@types/gapi.auth2": "0.0.50",
    "@types/jest": "^24.0.5",
    "@types/node": "^11.13.4",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "concurrently": "^4.1.0",
    "http-proxy-middleware": "^0.19.1",
    "nodemon": "^1.18.11",
    "ts-node": "^8.1.0",
    "typescript": "^3.3.1"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true
  }
}
